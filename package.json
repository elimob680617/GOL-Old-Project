{
  "name": "@gardenoflove/application",
  "author": "minimals.cc",
  "version": "3.0.0",
  "description": "next.js & typescript",
  "private": true,
  "scripts": {
    "lint": "next lint",
    "lint:es": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --fix --ext .ts,.tsx .",
    "dev": "next dev -p 8383",
    "start": "next start",
    "build": "set NODE_OPTIONS=--max-old-space-size=8192 && next build",
    "build-netlify": "next build && cp -r .next _next && mv _next .next/",
    "export": "yarn build && next export -o _static",
    "clean": "rm -rf node_modules .next/ coverage/",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "codegen": "set NODE_OPTIONS=--max-old-space-size=8192  && npm run gql-schemas && npm run cognito-hooks  && npm run profile-hooks  && npm run connection-hooks && npm run locality-hooks && npm run upload-hooks  && npm run chat-hooks && npm run post-behavior-hooks && npm run post-hooks && npm run search-hooks && npm run history-hooks",
    "gql-schemas": "graphql-codegen --config ./config/schemas.yml -r dotenv/config",
    "connection-hooks": "graphql-codegen --config ./config/connection-hooks.yml -r dotenv/config",
    "cognito-hooks": "graphql-codegen --config ./config/cognito-hooks.yml -r dotenv/config",
    "post-behavior-hooks": "graphql-codegen --config ./config/post-behavior-hooks.yml -r dotenv/config",
    "profile-hooks": "graphql-codegen --config ./config/profile-hooks.yml -r dotenv/config",
    "post-hooks": "graphql-codegen --config ./config/post-hooks.yml -r dotenv/config",
    "search-hooks": "graphql-codegen --config ./config/search-hooks.yml -r dotenv/config",
    "locality-hooks": "graphql-codegen --config ./config/locality-hooks.yml -r dotenv/config",
    "upload-hooks": "graphql-codegen --config ./config/upload-hooks.yml -r dotenv/config",
    "chat-hooks": "graphql-codegen --config ./config/chat-hooks.yml -r dotenv/config",
    "history-hooks": "graphql-codegen --config ./config/history-hooks.yml -r dotenv/config",
    "build:analyze": "ANALYZE=true next build",
    "prepare": "yarn global add git-branch-is && yarn global add husky && husky install",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build"
  },
